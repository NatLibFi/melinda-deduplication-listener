{
  "name": "melinda-deduplication-changelistener",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "cd melinda-deduplication-common && npm install",
    "build": "npm run build:main && npm run build:common",
    "build:main": "cd src && babel . -d ../build/ && cp ../package.json ../build",
    "build:common": "babel melinda-deduplication-common --ignore node_modules,spec.js --out-dir build/melinda-deduplication-common --source-maps && cp melinda-deduplication-common/package.json build/melinda-deduplication-common/package.json",
    "test": "mocha *.spec.js",
    "test:watch": "mocha *.spec.js -w",
    "flow": "flow",
    "dev": "nodemon -w build build/index.js"
  },
  "author": "Pasi Tuominen <pasi.tuominen@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.1.1",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.6.0",
    "flow-bin": "^0.48.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "aleph-change-listener": "git+https://github.com/NatLibFi/aleph-change-listener.git#eeede234679ddb69c175e9595f5ec389114e83a5",
    "debug": "^2.6.8",
    "lodash": "^4.17.4",
    "oracledb": "^1.13.1"
  }
}
